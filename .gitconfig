[core]
	editor = vim
[alias]
	a = add -A
	s = status
	c = commit
    ca = !git add -A && git commit -av
	p = pull origin master
	ps = push origin master
	go = checkout -B
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset' --abbrev-commit
[credential]
	helper = osxkeychain
